{
  "name": "evexi",
  "description": "Evexi Player API for file system, interactive and interactive scan library.",
  "version": "2.4.0",
  "repository": "https://github.com/MRXTechnology/Evexi.git",
  "author": "Peter Ingram <p.ingram@mrx.technology>",
  "license": "MIT",
  "types": "package/index.d.ts",
  "main": "package/index.js",
  "module": "src/index.ts",
  "scripts": {
    "build": "yarn run clean && yarn run scan:build && yarn run examples:build",
    "examples:build:local": "yarn run clean && yarn run example:build:fs:local && yarn run example:build:interactive:local && yarn run example:build:kiosk:local",
    "example:build:fs:local": "bash ./.build/buildExamplesLocal.sh fs fs/src",
    "example:build:interactive:local": "bash ./.build/buildExamplesLocal.sh interactive interactive/content/src",
    "example:build:kiosk:local": "bash ./.build/buildExamplesLocal.sh kiosk kiosk/src",
    "examples:build": "yarn run example:build:fs && yarn run example:build:interactive && yarn run example:build:scan && yarn run example:build:kiosk",
    "example:build:fs": "bash ./.build/buildExamples.sh fs fs/src",
    "example:build:interactive": "bash ./.build/buildExamples.sh interactive interactive/content/src",
    "example:build:scan": "bash ./.build/buildScanExample.sh",
    "example:build:kiosk": "bash ./.build/buildExamples.sh kiosk kiosk/src",
    "scan:test": "jest",
    "build:packages": "bash ./.build/buildPackages.sh",
    "scan:serve": "node_modules/.bin/parcel serve scan docs/interactive/scan/src/index.html",
    "fs:serve": "node_modules/.bin/parcel serve fs docs/fs/src/index.html",
    "clean": "bash ./.build/clean.sh",
    "check-types": "tsc --noemit --skipLibCheck"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-stage-0": "^7.8.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.2",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "dts-bundle-generator": "^5.9.0",
    "jest": "^27.0.6",
    "mock-socket": "^9.0.3",
    "parcel-bundler": "^1.12.5",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  }
}
